{
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run prettier"
    ],
    "*.ts": [
      "npm run lint:fix",
      "npm run prettier"
    ],
    "*.jsx": [
      "npm run lint:fix",
      "npm run prettier"
    ],
    "*.tsx": [
      "npm run lint:fix",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@reduxjs/toolkit": "^2.2.7",
    "framer-motion": "^11.3.29",
    "next": "^14.2.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-textarea-autosize": "^8.5.3",
    "sass": "^1.77.8"
  },
  "devDependencies": {
    "@types/node": "22.5.0",
    "@types/react": "18.3.4",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "typescript": "5.5.4"
  }
}
